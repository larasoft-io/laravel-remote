{
    "name": "larasoft/laravel-remote",
    "type": "library",
    "description": "Manage your Laravel Applications remotely.",
    "keywords": [
        "Larasoft",
        "LaravelRemote",
        "framework",
        "laravel"
    ],
    "homepage": "https://github.com/Larasoft/LaravelRemote",
    "license": "MIT",
    "authors": [
        {
            "name": "Larasoft.io",
            "email": "rock.star9722@gmail.com",
            "homepage": "https://larasoft.io/"
        }
    ],
    "require": {
        "illuminate/config": "5.1.* || 5.2.* || 5.3.*",
        "illuminate/database": "5.1.* || 5.2.* || 5.3.*",
        "illuminate/support": "5.1.* || 5.2.* || 5.3.*",
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Larasoft\\LaravelRemote\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larasoft\\LaravelRemote\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
